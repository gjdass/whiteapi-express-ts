{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "arrays": "never"
                }
            }
        ]
    },
    "rulesDirectory": []
}